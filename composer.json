{
    "name": "moonshine/quill",
    "description": "Quill editor for MoonShine",
    "keywords": ["moonshine", "quill"],
    "type": "library",
    "homepage": "https://moonshine.cutcode.dev",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/moonshine-software/quill/issues",
        "source": "https://github.com/moonshine-software/quill"
    },
    "authors": [
        {
            "name": "Danil Shutsky",
            "email": "info@cutcode.ru",
            "homepage": "https://cutcode.dev"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.8",
        "mockery/mockery": "^1.4.4",
        "phpstan/phpstan": "^1.4.7",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0",
        "brianium/paratest": "^6.8"
    },
    "autoload": {
        "psr-4": {
            "MoonShine\\Quill\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MoonShine\\Quill\\Tests\\": "tests/",
            "MoonShine\\Quill\\Database\\Factories\\": "database/factories/"
        }
    },
    "conflict": {
        "moonshine/moonshine": "<1.52"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MoonShine\\Quill\\Providers\\QuillServiceProvider"
            ]
        }
    }
}
